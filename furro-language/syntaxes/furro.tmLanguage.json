{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "furro",
	"scopeName": "source.furro",
  "patterns": [
    {
      "match": "\\b(Scream|Wait)\\b",
      "name": "keyword.control.furro"
    },
    {
      "match": "\"[^\"]*\"",
      "name": "string.quoted.double.furro"
    },
    {
      "match": "\\b(Hurray_End)\\b",
      "name": "support.function.furro"
    },
    {
      "match": "\\b(Protogen.Plus|Protogen.DivBy|Protogen.Minus|Protogen.Times)\\b",
      "name": "keyword.control.furro"
    },
    {
      "match": "(7u7).*$",
      "name": "comment.line.furro"
    },
    {
      "match": "\\b(7u7)\\b",
      "name": "support.function.furro"
    },
    {
      "match": "\\b\\d+\\b",
      "name": "constant.numeric.furro"
    },
    {
      "match": "\\b(Scream|Wait)\\b",
      "name": "keyword.control.furro"
    },
    {
      "match": "\\b\\d+\\b",
      "name": "constant.numeric.furro"
    }
  ]
}